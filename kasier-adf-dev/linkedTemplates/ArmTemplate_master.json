{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kasier-adf-dev"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"adls_kasiercovid_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_kasiercovid_ls'"
		},
		"blob_storagecovidudemy_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blob_storagecovidudemy_ls'"
		},
		"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kaiser-healthcare-kv.vault.azure.net/"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "practicesserver.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "practice_db"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ayyappa"
		},
		"adls_kasiercovid_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kaiseradlsdev.dfs.core.windows.net/"
		},
		"http_population_covid_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"event_trgr_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4c8088ef-4038-45a0-b326-85e0498c75ab/resourceGroups/kasier_dev/providers/Microsoft.Storage/storageAccounts/kaiseradlsdev"
		},
		"evtn_based_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4c8088ef-4038-45a0-b326-85e0498c75ab/resourceGroups/kasier_dev/providers/Microsoft.Storage/storageAccounts/kaiseradlsdev"
		},
		"population_ingestion_trgr_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4c8088ef-4038-45a0-b326-85e0498c75ab/resourceGroups/kasier_dev/providers/Microsoft.Storage/storageAccounts/storagecovidudemy"
		},
		"tumblinggg_properties_pipeline_parameters_pendate": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndtime"
		},
		"tumblinggg_properties_pipeline_parameters_pstartdate": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowstarttime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kasier-adf-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"adls_kasiercovid_ls_accountKey": {
						"value": "[parameters('adls_kasiercovid_ls_accountKey')]"
					},
					"blob_storagecovidudemy_ls_connectionString": {
						"value": "[parameters('blob_storagecovidudemy_ls_connectionString')]"
					},
					"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_ls_properties_typeProperties_baseUrl')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"adls_kasiercovid_ls_properties_typeProperties_url": {
						"value": "[parameters('adls_kasiercovid_ls_properties_typeProperties_url')]"
					},
					"http_population_covid_ls_properties_typeProperties_url": {
						"value": "[parameters('http_population_covid_ls_properties_typeProperties_url')]"
					},
					"event_trgr_properties_typeProperties_scope": {
						"value": "[parameters('event_trgr_properties_typeProperties_scope')]"
					},
					"evtn_based_properties_typeProperties_scope": {
						"value": "[parameters('evtn_based_properties_typeProperties_scope')]"
					},
					"population_ingestion_trgr_properties_typeProperties_scope": {
						"value": "[parameters('population_ingestion_trgr_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "kasier-adf-dev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tumblinggg_properties_pipeline_parameters_pendate": {
						"value": "[parameters('tumblinggg_properties_pipeline_parameters_pendate')]"
					},
					"tumblinggg_properties_pipeline_parameters_pstartdate": {
						"value": "[parameters('tumblinggg_properties_pipeline_parameters_pstartdate')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'kasier-adf-dev_ArmTemplate_0')]"
			]
		}
	]
}